  AfterExternBlock: false
  BeforeCatch:     false
  BeforeElse:      false
  IndentBraces:    false
  SplitEmptyFunction: false
  SplitEmptyRecord: false
  SplitEmptyNamespace: false
ColumnLimit:     120
IndentWidth:     4
ReflowComments:  true
TabWidth:        4
UseTab:          Never
